    <!-- First activity -->

    <string name="splash_appname_msg">DORMƎB</string>
    <string name="splash_first_msg">Sleep Better: Our app is designed to help you sleep more easily.</string>
    <string name="splash_second_msg">Sound Choices: Personalize your experience by choosing from three types of relaxing sounds for sleep.</string>
    <string name="splash_third_msg">Easy to Use: Simple and intuitive interface so you can set up your sleep environment quickly.</string>
    <string name="splash_button_start">START EXPERIENCE</string>
    <string name="splash_check_show_again">Don\'t show this again</string>
